{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mkgtru-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/mkgtru-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/mkgtru-api",
  "projects": {
    "mkgtru-api": {
      "type": "application",
      "root": "apps/mkgtru-api",
      "entryFile": "main",
      "sourceRoot": "apps/mkgtru-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mkgtru-api/tsconfig.app.json"
      }
    },
    "mkgt-official-bot": {
      "type": "application",
      "root": "apps/mkgt-official-bot",
      "entryFile": "main",
      "sourceRoot": "apps/mkgt-official-bot/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mkgt-official-bot/tsconfig.app.json"
      }
    },
    "documentation-static": {
      "type": "application",
      "root": "apps/documentation-static",
      "entryFile": "main",
      "sourceRoot": "apps/documentation-static/src",
      "compilerOptions": {
        "tsConfigPath": "apps/documentation-static/tsconfig.app.json"
      }
    }
  }
}